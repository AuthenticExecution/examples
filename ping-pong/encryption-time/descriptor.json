{
    "nodes": [
        {
            "type": "sgx",
            "name": "node_sgx",
            "host": "node-sgx",
            "reactive_port": 5000,
            "aesm_host": "aesm-client"
        },
        {
            "type": "native",
            "name": "node_native",
            "host": "node-native",
            "reactive_port": 5000
        }
    ],
    "modules" : [
        {
            "type": "sgx",
            "name": "ping",
            "node": "node_sgx",
            "vendor_key": "cred/vendor_key.pem",
            "ra_settings": "cred/settings.json",
            "features": ["measure_time"]
        },
        {
            "type": "native",
            "name": "pong",
            "node": "node_native",
            "features": ["measure_time"]
        }
    ],
    "connections": [
      {
          "from_module": "ping",
          "from_output": "send_ping",
          "to_module": "pong",
          "to_input": "recv_ping",
          "encryption": "spongent"
      },
      {
          "from_module": "pong",
          "from_output": "send_pong",
          "to_module": "ping",
          "to_input": "recv_pong",
          "encryption": "spongent"
      }
    ]
}
